
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using DüWEB.Models
@model List<Senate>

<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
</head>

<h3>Senato</h3>
<br />
<a href="/AdminManagement/AddSenate">
    <input type="button" value="Yeni Üye Ekle">
</a>
<table class="table table-bordered mt-3">
    <tr>
        <th>Id</th>
        <th>Görev</th>
        <th>Ünvan</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Güncelle</th>
        <th>Sil</th>


    </tr>

    @foreach (var senate in Model)
    {
<tr>
    <td>@senate.Id</td>
    <td>@senate.Title</td>
    <td>@senate.Degree</td>
    <td>@senate.Name</td>
    <td>@senate.Surname</td>
    <td><a href="/AdminManagement/SenateUpdate/@senate.Id" class="btn btn-primary">Güncelle</a></td>
    <td><a href="/AdminManagement/DeleteSenate/@senate.Id" class="btn btn-danger">Sil</a></td>

</tr>
    }




</table>

<h3>Yönetim Kurulu</h3>

<table class="table table-bordered mt-3">
    <tr>
        <th>Id</th>
        <th>Görev</th>
        <th>Ünvan</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Güncelle</th>
        <th>Sil</th>


    </tr>

    @foreach (var senate in Model)
    {
        if (senate.Id == 17)
        {
            break;
        }
<tr>
    <td>@senate.Id</td>
    <td>@senate.Title</td>
    <td>@senate.Degree</td>
    <td>@senate.Name</td>
    <td>@senate.Surname</td>
    <td><a href="/AdminManagement/SenateUpdate/@senate.Id" class="btn btn-primary">Güncelle</a></td>
    <td><a href="/AdminManagement/DeleteSenate/@senate.Id" class="btn btn-danger">Sil</a></td>

</tr>
    }




</table>